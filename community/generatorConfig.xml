                             implementationPackage="com.goochou.p2b.dao.impl" targetProject="bulls-model/src/main/java"
                             type="XMLMAPPER"/> -->

        <table schema="" tableName="user">
        	 <generatedKey column="id" sqlStatement="Mysql" identity="true" />
        	
        </table>
    </context>

</generatorConfiguration>